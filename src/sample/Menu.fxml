<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MenuController">
   <children>
      <Button fx:id="backMenu" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onMouseClicked="#backToLogIn" text="Go Back" textFill="#2e00fc">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Text fx:id="meetingText" fill="#f89807" layoutX="135.0" layoutY="45.0" strokeType="OUTSIDE" strokeWidth="0.0" textAlignment="CENTER" wrappingWidth="309.13671875">
         <font>
            <Font name="System Bold" size="23.0" />
         </font>
      </Text>
      <TableView fx:id="table" layoutX="175.0" layoutY="107.0" prefHeight="286.0" prefWidth="259.0">
        <columns>
          <TableColumn fx:id="id" prefWidth="37.0" text="ID" />
          <TableColumn fx:id="type_name" prefWidth="160.0" text="Название" />
            <TableColumn fx:id="discount" prefWidth="48.0" text="Скидка" />
        </columns>
      </TableView>
      <Button fx:id="pick" layoutX="217.0" layoutY="403.0" mnemonicParsing="false" onMouseClicked="#goToProducts" prefHeight="49.0" prefWidth="167.0" text="Выбрать" />
      <Text fill="#f89807" layoutX="150.0" layoutY="101.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Категории товаров" textAlignment="CENTER" wrappingWidth="309.13671875">
         <font>
            <Font name="System Bold" size="23.0" />
         </font>
      </Text>
      <Button fx:id="OrdersButton" layoutX="194.0" layoutY="13.0" mnemonicParsing="false" onMouseClicked="#goToOrders" prefHeight="31.0" prefWidth="214.0" text="МОИ ЗАКАЗЫ" textFill="#ff6804">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
