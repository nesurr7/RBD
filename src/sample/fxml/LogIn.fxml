<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LogInController">
   <children>
      <ImageView fx:id="login_image" fitHeight="109.0" fitWidth="255.0" layoutX="121.0" layoutY="181.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../rsc/login.png" />
         </image>
      </ImageView>
      <Button defaultButton="true" layoutX="394.0" layoutY="406.0" mnemonicParsing="false" onMouseClicked="#loginIn" prefHeight="49.0" prefWidth="208.0" text="Вход">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Button>
      <ImageView fitHeight="96.0" fitWidth="244.0" layoutX="145.0" layoutY="285.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../rsc/password.png" />
         </image>
      </ImageView>
      <TextField fx:id="loginArea" layoutX="376.0" layoutY="196.0" prefHeight="54.0" prefWidth="244.0">
         <font>
            <Font size="33.0" />
         </font></TextField>
      <TextField fx:id="passwordArea" layoutX="376.0" layoutY="297.0" prefHeight="54.0" prefWidth="244.0">
         <font>
            <Font size="33.0" />
         </font></TextField>
      <Text fx:id="textSignal" layoutX="127.0" layoutY="131.0" strokeType="OUTSIDE" strokeWidth="0.0" textAlignment="CENTER" wrappingWidth="742.0">
         <font>
            <Font size="59.0" />
         </font>
      </Text>
      <Button defaultButton="true" layoutX="322.0" layoutY="497.0" mnemonicParsing="false" onMouseClicked="#register" prefHeight="69.0" prefWidth="353.0" text="Зарегистрироваться">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Button>
   </children>
   <cursor>
      <Cursor fx:constant="HAND" />
   </cursor>
</AnchorPane>
